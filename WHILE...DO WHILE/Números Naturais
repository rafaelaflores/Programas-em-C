/*Faça um programa que leia um número inteiro positivo N e imprima todos os números naturais de 0 até N em ordem crescente.*/ 
 #include <stdio.h>
 #include <stdlib.h>
 #include <locale.h>
	int main() {
	setlocale(LC_ALL, "Portuguese");
	int x=0, n;
	printf("Digite o número n, não negativo: ");
	scanf("%d", &n);
	printf("Os números naturais de 0 a %d em ordem crescente são: ",n);
	while (x <= n) {
	printf("%d ", x);
	x++;
}

	return 0;
	}
