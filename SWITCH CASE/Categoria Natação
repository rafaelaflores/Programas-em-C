#include <stdlib.h>
#include <stdio.h>
#include <locale.h>
#include <conio.h>
	
	int main()
	{
		setlocale(LC_ALL, "Portuguese");
		int idade;
	
		printf("Digite a sua idade, a partir de 5 anos: ");
		scanf("%d",&idade);
		
			switch(idade)
			{
				case (0)...(4):
						printf("\nInválido!\n");
				break;
	
			case 5:
			case 6:
			case 7:
				printf("\nNadador(a), você pertence a categoria Infantil A!\n");
				break;
			case 8:
			case 9:
			case 10:
				printf("\nNadador(a), você pertence a categoria Infantil B!\n");
				break;
			case 11:
			case 12:
			case 13:
				printf("\nNadador(a), você pertence a categoria Juvenil A!\n");
				break;
			case 14:
			case 15:
			case 16:
			case 17:
				printf("\nNadador(a), você pertence a categoria Juvenil B!\n");
				break;
				
				default:
			printf("\nNadador(a), você pertence a categoria Sênior!\n");
			break;
			}
	printf("\n\n");
				system("pause");
			return 0; 
		
		
}
		
		
		
	
	
